{
    "name": "修改密码",
    "method": "POST",
    "params": {
        "title": {
            "rem": "邮箱或手机号",
            "need": true,
            "reg": "^(1\\d{10})|(([\\w-])+@([\\w-])+(.[\\w-])+)$"
        },
        "password": {
            "rem": "密码",
            "need": true,
            "len": [6, 32]
        },
        "code": {
            "rem": "验证码",
            "need": true,
            "reg": "^\\w{4,8}$"
        }
    },
    "error": {
        "405": "账号不存在",
        "406": "验证码错误",
        "407": "验证码已过期,请重新发送验证码"
    },
    "ret": {
        "no": 200,
        "msg": "尚未实现"
    }
}